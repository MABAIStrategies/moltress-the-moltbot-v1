{
  "name": "@moltbot/agent",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run --exclude tests/contract.test.ts --exclude tests/e2e.test.ts",
    "test:contract": "vitest run tests/contract.test.ts",
    "test:e2e": "vitest run tests/e2e.test.ts"
  },
  "dependencies": {
    "check-disk-space": "3.4.0",
    "cors": "2.8.5",
    "express": "4.19.2",
    "express-rate-limit": "7.4.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "22.10.5",
    "openapi-response-validator": "9.3.4",
    "openapi-schema-validator": "12.1.3",
    "supertest": "6.3.4",
    "tsx": "4.7.0",
    "typescript": "5.6.3",
    "vitest": "1.6.0",
    "yaml": "2.4.5"
  }
}
